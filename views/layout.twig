<!DOCTYPE html>
<html>
  <head>
    {% block head %}
      <meta charset="utf-8">
      <title>{% block title %}{% endblock %}</title>

      <link rel="stylesheet" href= {{ URL_CSS }}bootstrap.min.css>
    {% endblock %}
  </head>
  <body>
    <header>
    {% block header %}{% endblock %}
    </header>

    <div id="content">
      <div class="container">
        {% block content %}{% endblock %}
      </div>
    </div>
    <footer>
        {% block footer %}
        <script src="{{ URL_JS }}jquery.min.js" charset="utf-8"></script>
        <script src="{{ URL_JS }}popper.min.js" charset="utf-8"></script>
        <script src="{{ URL_JS }}bootstrap.min.js" charset="utf-8"></script>
        {% endblock %}
    </footer>
  </body>
</html>
